{
  "variables": {
    "upgrade.button": false,
    "upgrade.reload": false
  },
  "apps": {
    "palantir-docs": {
      "type": "pages",
      "config": {
        "directory": "/docs",
        "index": "README.md.html"
      }
    },
    "palantir-changelog": {
      "type": "timeline",
      "config": {
        "url": "/changelog/changelog.json",
        "capitalizeDomains": true,
        "datesTitle": "Versions...",
        "filterText": "Filter..."
      }
    }
  },
  "menu": [{
    "id": "menu-palantir",
    "title": "Palantir",
    "sections": [{
      "id": "section-palantir",
      "title": "Palantir",
      "links": [{
        "root": true,
        "id": "link-palantir-docs",
        "title": "Documentations",
        "icon": "fas fa-book",
        "app": "palantir-docs"
      }, {
        "id": "link-palantir-changelog",
        "title": "Changelog",
        "icon": "fas fa-stream",
        "app": "palantir-changelog"
      }]
    }]
  }]
}
