{
  "name": "palantir",
  "version": "0.1.4",
  "private": true,
  "bin": {
    "palantir": "src/cli/index.js"
  },
  "scripts": {
    "serve": "vue-cli-service serve ./src/bootstrap/index.js",
    "build": "vue-cli-service build ./src/bootstrap/index.js",
    "build:watch": "vue-cli-service build --watch ./src/bootstrap/index.js",
    "lint": "vue-cli-service lint",
    "start": "node ./src/server/index.js",
    "storybook": "start-storybook -p 9000 -c ./src/bootstrap/storybook",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "npm run changelog && git add CHANGELOG.md",
    "release": "npm version patch -m 'release: %s'"
  },
  "dependencies": {
    "cli-color": "^1.4.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "marked": "^0.5.2",
    "mkp": "^1.0.1",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^4.0.7",
    "@storybook/vue": "^4.0.7",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-preset-vue": "^2.0.2",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.10.0",
    "prismjs": "^1.15.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "babel": {
    "presets": [
      "@vue/app",
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "import-glob"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
