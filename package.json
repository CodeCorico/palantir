    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 9000 -c ./src/bootstrap/storybook",
    "changelog-to-timeline": "node ./scripts/changelog-to-timeline.js"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^4.0.7",
    "axios": "^0.18.0",
